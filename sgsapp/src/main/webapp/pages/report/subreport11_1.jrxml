	<queryString>
		<![CDATA[SELECT
     SEG_REL_CUESTIONARIO.N_COD_INSTELEFONICA AS N_COD_INSTELEFONICA,
     SEG_DET_PREGUNTA.N_COD_PREGUNTA AS N_COD_PREGUNTA,
     SEG_DET_PREGUNTA.N_COD_EMPRESA AS N_COD_EMPRESA,
     SEG_DET_PREGUNTA.V_DESCRIPCION AS V_DESCRIPCION,
     SEG_DET_PREGUNTA.N_FLG_ACTIVO AS N_FLG_ACTIVO,
     SEG_DET_PREGUNTA.V_USU_CREACION AS V_USU_CREACION,
     to_char(SEG_DET_PREGUNTA.D_FEC_CREACION,
        'dd/mm/yyyy hh24:mi:ss') AS D_FEC_CREACION,
     SEG_DET_PREGUNTA.V_IP_CREACION AS V_IP_CREACION,
     SEG_DET_PREGUNTA.V_USU_MODIFICACION AS V_USU_MODIFICACION,
     to_char(SEG_DET_PREGUNTA.D_FEC_MODIFICACION,
        'dd/mm/yyyy hh24:mi:ss') AS D_FEC_MODIFICACION,
     SEG_DET_PREGUNTA.V_IP_MODIFICACION AS V_IP_MODIFICACION     
FROM
     SGSWEB.SEG_DET_PREGUNTA SEG_DET_PREGUNTA,
     SGSWEB.SEG_REL_CUESTIONARIO SEG_REL_CUESTIONARIO
WHERE
     SEG_REL_CUESTIONARIO.N_COD_PREGUNTA = SEG_DET_PREGUNTA.N_COD_PREGUNTA
AND  SEG_REL_CUESTIONARIO.N_COD_EMPRESA = SEG_DET_PREGUNTA.N_COD_EMPRESA
AND  SEG_REL_CUESTIONARIO.N_COD_INSTELEFONICA = $P{N_COD_INSTELEFONICA}
ORDER BY SEG_DET_PREGUNTA.N_COD_PREGUNTA]]>
